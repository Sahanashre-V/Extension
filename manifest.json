{
    "manifest_version": 3,
    "name": "My First Extension",
    "version": "1.0.0",
    "icons": {
        "32": "exten.png"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_icon": "exten.png",
        "default_title": "Click me!"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}